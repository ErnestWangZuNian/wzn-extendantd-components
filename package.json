{
  "name": "wzn-extendantd-components",
  "version": "0.0.9",
  "description": "",
  "main": "lib/indexjs",
  "module": "es/index.js",
  "browserslist": [
    "last 2 versions",
    "Android >= 4.0",
    "Firefox ESR",
    "not ie < 9"
  ],
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:all": "rm -rf dist lib es .docz",
    "clean": "rm -rf dist lib es .docz && mkdir dist lib es",
    "clean:lib": "rm -rf lib  && mkdir lib",
    "clean:umd": "rm -rf dist && mkdir dist",
    "clean:es": "rm -rf es && mkdir es",
    "start": "cross-env webpack-dev-server --open --config webpack.dev.js",
    "build": "cross-env npm run clean && npm run build:umd && npm run build:lib && npm run build:es && npm run build:css",
    "build:css": "gulp",
    "build:umd": "cross-env npm run clean:umd && webpack --config webpack.prod.js",
    "build:lib": "cross-env npm run clean:lib && cross-env OUTPUT_MODULE=commonjs babel src/components -d lib --ignore **/__tests__",
    "build:es": "cross-env npm run clean:es && babel src/components -d es --ignore **/__tests__",
    "release": "npm version patch && npm run build && npm publish --access=public",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "lint": "eslint --ext .jsx,.js src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErnestWangZuNian/wzn-extendantd-components.git"
  },
